{
  "name": "strumkey",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "postinstall": "mkdir -p public && cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/pdf.worker.min.mjs || true",
    "dev": "node scripts/dev.js",
    "dev:clean": "node scripts/dev.js --clean",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "sync-schema": "npx instant-cli@latest push schema --app $(grep VITE_INSTANTDB_APP_ID .env | cut -d '=' -f2) -y",
    "sync-perms": "npx instant-cli@latest push perms --app $(grep VITE_INSTANTDB_APP_ID .env | cut -d '=' -f2) -y",
    "sync-all": "npx instant-cli@latest push all --app $(grep VITE_INSTANTDB_APP_ID .env | cut -d '=' -f2) -y",
    "watch-sync": "node scripts/watch-sync.js",
    "clear-groups": "npx tsx scripts/clear-groups.ts",
    "clean:cache": "rm -rf node_modules/.vite .vite",
    "clean:all": "npm run clean:cache && rm -rf dist node_modules/.cache",
    "deploy:staging": "git push origin develop",
    "deploy:production": "node scripts/deploy-production.js"
  },
  "dependencies": {
    "@instantdb/admin": "^0.22.96",
    "@instantdb/react": "^0.22.96",
    "@phosphor-icons/react": "^2.1.10",
    "jspdf": "^2.5.1",
    "pdfjs-dist": "^5.4.530",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ios-keyboard-viewport": "latest",
    "react-router-dom": "^6.20.0",
    "tesseract.js": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.8"
  }
}
